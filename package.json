{
  "name": "cra",
  "version": "0.0.0",
  "scripts": {
    "# general commands": "##################################################",
    "build": "react-scripts build",
    "deduplicate": "npx yarn-deduplicate && yarn",
    "eject": "react-scripts eject",
    "lint": "eslint --fix \"./**/*.{js,ts}\"",
    "start": "react-scripts start",
    "validate": "yarn lint",
    "#": "###################################################################"
  },
  "dependencies": {
    "@babel/eslint-plugin": "^7.14.5",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "react": "^17",
    "react-dom": "^17",
    "react-hook-form": "^7.17.5",
    "react-icons": "^4.3.1",
    "@types/react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0",
    "@types/react-router-dom": "^5.3.0",
    "react-scripts": "^4",
    "typescript": "^4.1.2",
    "styled-components": "^5.3.3",
    "@types/styled-components": "^5.3.3"
  },
  "devDependencies": {
    "eslint": "^7",
    "eslint-config-kdnj": "^1.0.0-2",
    "prettier": "2.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kdnj/react"
    ],
    "rules": {
      "simple-import-sort/imports": 0,
      "simple-import-sort/exports": 0,
      "no-unused-vars": "warn",
      "@typescript-eslint/no-unused-vars": ["warn"]
    },
    "root": true
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "private": true
}
